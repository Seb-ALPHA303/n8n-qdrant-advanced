{
  "name": "n8n-nodes-qdrant-advanced",
  "version": "0.1.0",
  "description": "Advanced Qdrant community node for n8n â€” full Qdrant HTTP API support with $fromAI expressions",
  "keywords": ["n8n-nodes", "qdrant", "community", "vector", "embedding"],
  "license": "MIT",
  "author": "Your Name <you@example.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/<your-username>/n8n-nodes-qdrant-advanced.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      { "name": "qdrantApi", "type": "QdrantApi" }
    ],
    "nodes": [
      { "name": "Qdrant (Advanced)", "type": "qdrantAdvanced", "file": "dist/index.js" }
    ]
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.0.0",
    "n8n-core": "^0.200.0",
    "n8n-workflow": "^0.200.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "eslint-plugin-jsdoc": "^44.0.0",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
